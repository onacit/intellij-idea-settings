<keymap version="1" name="Emacs copy" parent="Emacs">
  <action id="NextProjectWindow">
    <keyboard-shortcut first-keystroke="ctrl x" second-keystroke="ctrl o" />
    <keyboard-shortcut first-keystroke="meta back_quote" />
  </action>
  <action id="OverrideMethods">
    <keyboard-shortcut first-keystroke="ctrl meta o" />
  </action>
  <action id="PreviousProjectWindow">
    <keyboard-shortcut first-keystroke="shift meta back_quote" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl back_slash" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl meta r" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="shift ctrl meta r" />
  </action>
</keymap>