<templateSet group="Java">
  <template name="tic" value="@org.junit.jupiter.api.TestInstance(org.junit.jupiter.api.TestInstance.Lifecycle.PER_CLASS)" description="@TestInstance(PER_CLASS)" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="ms" value="@org.junit.jupiter.params.provider.MethodSource({&quot;$VARIABLE$&quot;})" description="@MethodSource({&quot;&quot;})" toReformat="true" toShortenFQNames="true">
    <variable name="VARIABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="at" value="org.assertj.core.api.Assertions.assertThat($VARIABLE$)" description="@MethodSource({&quot;&quot;})" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="VARIABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="attb" value="org.assertj.core.api.Assertions.assertThatThrownBy(() -&gt; $VARIABLE$)" description="assertThatThrownBy" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="VARIABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="atc" value="org.assertj.core.api.Assertions.assertThatCode(() -&gt; {$VARIABLE$})" description="assertThatCode" toReformat="true" toShortenFQNames="true" useStaticImport="true">
    <variable name="VARIABLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="done" value="// DONE: $VAR$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>